{
  "name": "collapse-all-artboards-and-groups",
  "description": "Collapse all artboards and groups in current document",
  "version": "1.0.2",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Collapse All Artboards and Groups",
    "manifest": "src/manifest.json",
    "main": "Collapse All Artboards and Groups.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "littlebusters"
}
